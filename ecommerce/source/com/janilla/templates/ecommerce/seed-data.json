	"pages": [
		{
			"$type": "Page",
			"id": 1,
			"title": "Home",
			"hero": {
				"$type": "Hero",
				"type": {
					"$type": "Hero.Type",
					"name": "HIGH_IMPACT"
				},
				"richText": "<h1>Janilla Ecommerce Template</h1><p><a href=\"/admin\">Visit the admin dashboard</a> to begin managing this site's content. The code for this template is completely open-source and can be found <a href=\"https://github.com/diego-schivo/janilla-templates/tree/main/website\" rel=\"noopener noreferrer\" target=\"_blank\">on our Github</a>. </p>"
			},
			"layout": [
				{
					"$type": "Content",
					"columns": [
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "FULL"
							},
							"richText": "<h2>Core features</h2>"
						},
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "ONE_THIRD"
							},
							"richText": "<h3>Admin Dashboard</h3><p>Manage this site's pages and products from the <a href=\"/admin\">admin dashboard</a>.</p>"
						},
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "ONE_THIRD"
							},
							"richText": "<h3>Preview</h3><p>Using versions, drafts, and preview, editors can review and share their changes before publishing them.</p>"
						},
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "ONE_THIRD"
							},
							"richText": "<h3>Page Builder</h3><p>Custom page builder allows you to create unique page, product, and project layouts for any type of content.</p>"
						},
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "ONE_THIRD"
							},
							"richText": "<h3>SEO</h3><p>Editors have complete control over SEO data and site content directly from the <a href=\"/admin\">admin dashboard</a>.</p>"
						},
						{
							"$type": "Column",
							"size": {
								"$type": "Column.Size",
								"name": "ONE_THIRD"
							},
							"richText": "<h3>Dark Mode</h3><p>Users will experience this site in their preferred color scheme and each block can be inverted.</p>"
						}
					]
				},
				{
					"$type": "MediaBlock",
					"media": 2
				},
				{
					"$type": "CallToAction",
					"richText": "<h3>This is a call to action</h3><p>This is a custom layout building block <a href=\"/admin\">configured in the admin dashboard</a>.</p>",
					"links": [
						{
							"$type": "Link",
							"type": {
								"$type": "Link.Type",
								"name": "CUSTOM"
							},
							"uri": "/shop",
							"text": "All products",
							"appearance": {
								"$type": "Link.Appearance",
								"name": "DEFAULT"
							}
						}
					]
				}
			],
